{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "category": "web_framework",
      "component": "node"
    },
    "name": "node"
  },
  "spec": {
    "ports": [
      {
        "name": "http-port",
        "port": 3000,
        "protocol": "TCP",
        "targetPort": "http-port"
      }
    ],
    "selector": {
      "app": "node"
    },
    "sessionAffinity": "None",
    "type": "LoadBalancer"
  }
}